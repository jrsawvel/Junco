/* sidr-related css */

#mobile-header {
    display: block; 
    /* float: right; */
    margin: 0;
    padding: 0;
    background-color: transparent;
    z-index: 9999; 
    /* use 'position: fixed' if desire to pin hamburger icon */
    position: fixed;
    width: 100%;
    text-align: right;
    line-height: 1em;
}

#navigation {
    display: none;
}

.sidr {
    width: 160px;
    background: #666666;
}

.sidr ul li:hover {
    background-color: #777777;
    
}

.sidr ul li a, .sidr ul li span {
    color: #cccccc;
}

.sidr ul {
    border-top: none;
    border-bottom: none;
}

.sidr ul li {
    border-top: none;
    border-bottom: none;
}

#responsive-menu-button {
    background: #fff;
}

/* some hamburger tips from:
http://www.ymc.ch/en/how-to-make-a-hamburger-a-menu-for-mobile-websites
*/

#hamburger {
    padding: 5px 5px 0 5px;
    width: 15px;
    float: right;
}

#hamburger div {
    background-color: #ccc;
    height: 2px;
    width: 90%;
    margin-bottom: 2px; 
}

#content {
    clear: right; 
/*
    margin: 0px auto 0px;
    position: relative;
    max-width: 42em; 
*/
}

#navigation {
    margin: 0;
    padding: 0;
}

.nav {
    margin: 0;
    padding: 0;
} 

h1 {
    line-height: 1em;
}

@media only screen and (max-width: 45em) { 
    #mobile-header {
        position: absolute;
    }
}


